{
    "name": "n8n-nodes-stencilpdf",
    "version": "0.1.3",
    "description": "Node to interact with stencils rest API",
    "license": "MIT",
    "homepage": "stencilpdf.com",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Leon Kirschner",
        "email": "kirschner@hksolutions.at"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/trpouh/n8n-nodes-stencilpdf.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/StencilpdfApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/stencilpdf/Stencilpdf.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^25.0.10",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
